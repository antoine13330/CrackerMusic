@keyframes in {
    from {
        opacity: 0;
        transform: translateY(10px);
      }
      to {
        opacity: 1;
        transform: translateY(0);
      }
  }
  
  @keyframes slide-in {
    from {
        opacity: 0;
        transform: translate(-40px, 10px);
      }
      to {
        opacity: 1;
        transform: translate(0,0);
      }
  }
  
